{
  "name": "GCR AI",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered autonomous trading agent for Aster Finance",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "type-check": "pnpm -r type-check",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
